
Table fillTable(String s, Num n) {

    Table table = newTable(n, n);


    for(Num i = 0; i < n; i = i + 1) {
        for(Num j = 0; j < n; j = j + 1) {
            table[i][j] = s;
        }
    }

    return table;
}

String main() {

    Num sizeOfTable = 3;

    String s = "Hello " + "World" + "!!!";
    print fillTable(s, sizeOfTable);


}